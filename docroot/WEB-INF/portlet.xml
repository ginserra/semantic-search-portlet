<?xml version="1.0"?>

<portlet-app
	version="2.0"
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
>
	<portlet>
		<portlet-name>SemanticSearch-portlet</portlet-name>
		<display-name>SemanticSearch-portlet</display-name>
		<portlet-class>it.infn.ct.SemanticSearch_portlet</portlet-class>
		<init-param>    
			<name>portletVersion</name>
			<value>1.0</value>
		</init-param> 
		<init-param>
			<name>logLevel</name>
			<value>info</value>
		</init-param>
		<init-param>    
			<name>bdiiHost</name>
			<value>ldap://bdii.eumedgrid.eu:2170</value>
		</init-param>
		<init-param>    
			<name>wmsHost</name>
			<value>wms://wms-4.dir.garr.it:7443/glite_wms_wmproxy_server</value>
		</init-param>
		<init-param>
			<name>pxServerHost</name>
			<value>myproxy.ct.infn.it</value>
		</init-param>
		<init-param>
			<name>pxServerPort</name>
			<value></value>
			</init-param>
		<init-param>
			<name>pxServerSecure</name>
			<value></value>
		</init-param>
		<init-param>
			<name>pxRobotId</name>
			<value>21057</value>
		</init-param>
		<init-param>
			<name>pxRobotVO</name>
			<value>eumed</value>
		</init-param>
		<init-param>
			<name>pxRobotRole</name>
			<value>eumed</value>
		</init-param>
		<init-param>
			<name>pxUserProxy</name>
			<value></value>
		</init-param>
		<init-param>
			<name>pxRobotRenewalFlag</name>
			<value>true</value>
		</init-param>
		<init-param>
			<name>sciGwyAppId</name>
			<value>9</value>
		</init-param>
		<init-param>
			<name>sciGwyUserTrackingDB_Hostname</name>
			<value>localhost</value>
		</init-param>
		<init-param>
			<name>sciGwyUserTrackingDB_Username</name>
			<value>tracking_user</value>
		</init-param>
		<init-param>
			<name>sciGwyUserTrackingDB_Password</name>
			<value>usertracking</value>
		</init-param>
		<init-param>
			<name>sciGwyUserTrackingDB_Database</name>
			<value>userstracking</value>
		</init-param>
		<init-param>    
			<name>jobRequirements</name>
			<value></value>
		</init-param>            
		<init-param>    
			<name>pilotScript</name>
			<value></value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<!--<portlet-mode>edit</portlet-mode> -->
			<portlet-mode>help</portlet-mode>
		</supports>
		<portlet-info>
			<title>SemantiSearch-portlet</title>
			<short-title>SemanticSearch-portlet</short-title>
			<keywords>SemanticSearch-portlet</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>
