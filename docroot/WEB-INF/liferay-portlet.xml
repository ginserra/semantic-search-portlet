<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.0.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_0_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>SemanticSearch-portlet</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>true</instanceable>
                <header-portlet-css>/css/jquery-ui.css</header-portlet-css> 
		<header-portlet-css>/css/main.css</header-portlet-css>
                <header-portlet-css>/css/layoutInput.css</header-portlet-css>
                <header-portlet-css>/css/detailResource.css</header-portlet-css>
                <header-portlet-css>/css/layoutGraph.css</header-portlet-css>
                <header-portlet-css>/css/layoutPagination.css</header-portlet-css>
              
                
                <header-portlet-javascript>/js/main.js</header-portlet-javascript>
                <header-portlet-javascript>/js/jsGraph/jquery.min.js</header-portlet-javascript>
               
                <header-portlet-javascript>/js/jquery-1.9.1.js</header-portlet-javascript> 
                <header-portlet-javascript>/js/jquery-ui.js</header-portlet-javascript> 
                
                
                <footer-portlet-javascript>/js/tabber.js</footer-portlet-javascript>
                <footer-portlet-javascript>/js/jsGraph/RenderOntology14.js</footer-portlet-javascript>
                <footer-portlet-javascript>/js/jsGraph/arbor-tween.js</footer-portlet-javascript>
                <footer-portlet-javascript>/js/jsGraph/arbor.js</footer-portlet-javascript>
                <footer-portlet-javascript>/js/jsGraph/graphics.js</footer-portlet-javascript>
                
                <footer-portlet-javascript>/js/sigma-min.js</footer-portlet-javascript>
		<css-class-wrapper>SemanticSearch_portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>
